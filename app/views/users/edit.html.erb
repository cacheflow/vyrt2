<%=form_for edit_user_path do |u| %> 
  <%=u.text_field :name, placeholder: 'name' %> 
  <%=u.email_field :email, placeholder: 'email' %> 
  <%=u.text_field :age, placeholder: 'age' %> 
  <%=u.text_field :location, placeholder: 'location' %> 
  <%=u.hidden_field :created_at %> 
  <%=u.file_field :avatar %>
  <%=u.submit "Create User" %>  
<% end %> 

